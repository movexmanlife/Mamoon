<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MultiViewPager">
        <!-- Child view inside a page to match the page size against. -->
        <attr name="matchChildWidth" format="reference" />
        <!-- Maximum size. -->
        <attr name="android:maxWidth" />
        <!-- Maximum size. -->
        <attr name="android:maxHeight" />
    </declare-styleable>
    <declare-styleable name="ArcRateChart">
        <attr name="smallRateColor" format="color" />
        <attr name="smallRateArcWidth" format="dimension"></attr>
        <attr name="bigRateColor" format="color" />
        <attr name="bigRateArcWidth" format="dimension"></attr>
        <attr name="noRateColor" format="color" />
        <attr name="bgCircleColor" format="color" />
        <attr name="drawArcTime" format="integer"></attr>
        <attr name="rotateTime" format="integer"></attr>
    </declare-styleable>
</resources>